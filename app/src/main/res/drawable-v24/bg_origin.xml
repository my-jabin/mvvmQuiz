<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/navy_blue" />
        </shape>
    </item>

    <item
        android:left="350dp">
        <rotate
            android:fromDegrees="-45"
            android:pivotX="0%"
            android:pivotY="0%">
            <shape android:shape="rectangle">
                <solid android:color="@color/navy_blue_dark" />
            </shape>
        </rotate>
    </item>

    <item
        android:right="200dp">
        <rotate
            android:fromDegrees="-40"
            android:pivotX="100%"
            android:pivotY="100%">
            <shape android:shape="rectangle">
                <solid android:color="@color/yellow" />
            </shape>
        </rotate>
    </item>

    <item
        android:right="250dp">
        <rotate
            android:fromDegrees="-40"
            android:pivotX="100%"
            android:pivotY="100%">
            <shape android:shape="rectangle">
                <solid android:color="@color/red_dark" />
            </shape>
        </rotate>
    </item>

    <item
        android:left="300dp"
        android:top="60dp">
        <rotate
            android:fromDegrees="-40"
            android:pivotX="100%"
            android:pivotY="0%">
            <shape android:shape="rectangle">
                <solid android:color="@color/cyan_dark" />
            </shape>
        </rotate>
    </item>


    <item>
        <rotate
            android:fromDegrees="-40"
            android:pivotX="-10%"
            android:pivotY="100%">
            <shape android:shape="rectangle">
                <solid android:color="@color/light_blue" />
            </shape>
        </rotate>
    </item>


